    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "remix": "^1.1.1",
    "tailwindcss": "^3.0.11",
    "tiny-invariant": "^1.2.0",
    "zod": "^3.11.6"
  },
  "devDependencies": {
